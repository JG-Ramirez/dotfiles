{
  "codeLens.enable": false,
  "suggest.enablePreview": true,
  "diagnostic.virtualText": true,
  "suggest.maxCompleteItemCount": 20,
  "snippets.userSnippetsDirectory": "~/.config/nvim/ultisnips",
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "javascript",
    "json"
  ],
  "suggest.noselect": false,
  "eslint.packageManager": "yarn",
  "less.validate": true,
  "scss.validate": true,
  "less.lint.argumentsInColorFunction": "warning",
  "coc.preferences.extensionUpdateCheck": "daily",
  "emmet.extensionsPath": "~/.config/nvim/emmet_custom",
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showSuggestionsAsSnippets": false,
  "emmet.optimizeStylesheetParsing": false,
  "emmet.includeLanguages": {
    "liquid": "html",
    "html.twig": "html",
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "html": {
      "filters": "html, bem"
    }
  },
  "emmet.preferences": {
    "bem.elementSeparator": "__",
    "bem.modifierSeparator": "--",
    "bem.shortElementPrefix": "-"
  },
  "emmet.showAbbreviationSuggestions": true,
  "tailwindCSS.trace.server": "verbose",
  "tailwindCSS.headwind.runOnSave": false,
  "tailwindCSS.jsLanguages": [
    "javascript",
    "javascriptreact",
    "reason",
    "typescriptreact",
    "typescript.tsx"
  ]
}
